{
  "expo": {
    "name": "cal-tag-app",
    "slug": "cal-tag-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "your-app-scheme",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.parthchandak.caltagapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.parthchandak.caltagapp"
    },
    "web": {
      "favicon": "./assets/images/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ],
    "fonts": [
      {
        "asset": "assets/fonts/Montserrat-VariableFont_wght.ttf",
        "family": "Montserrat"
      },
      {
        "asset": "assets/fonts/Montserrat-Italic-VariableFont_wght.ttf",
        "family": "Montserrat-Italic"
      }
    ]
  }
}
